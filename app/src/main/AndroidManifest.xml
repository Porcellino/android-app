<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="porce.com.porce" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.MyPageActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.UploadActivity"
            android:label="@string/title_activity_upload"
            android:parentActivityName=".activities.MyPageActivity" >
        </activity>
        <activity
            android:name=".activities.UploadDetailActivity"
            android:label="@string/title_activity_upload_detail"
            android:parentActivityName=".activities.MyPageActivity" >
        </activity>
    </application>

</manifest>
